<h1>Carts#show</h1>

<%# render wokouts from the Carts join table %>

<%@workouts.each do |w|%>
    <div>
        <strong>Title: </strong>
        <%= w.title %>
        <br>
        <strong>Description: </strong>
        <%= w.description %>
        <br>
        <strong>Price: </strong>
        <%= w.price %>
        <br>
        <strong>Duration: </strong>
        <%= w.duration %>
    </div>
<%end%>